generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "foreignKeys"
}

model Category {
  id          Int           @id @default(autoincrement())
  title       String
  subcategory SubCategory[]
}

model SubCategory {
  id       Int      @id @default(autoincrement())
  title    String
  CatId    Int
  category Category @relation(fields: [CatId], references: [id])
}
